{
	"name" : "philkra/elastic-apm-laravel",
	"keywords": [
        "laravel",
        "elastic",
        "APM",
        "Lumen",
        "monitoring",
        "debug"
    ],
    "description": "A package to integrate Elastic APM into Laravel",
	"license": "MIT",
	"require" : {
		"php" : ">= 7.0",
		"illuminate/database": "5.5.x|5.6.x",
        "illuminate/http": "5.5.x|5.6.x",
        "illuminate/routing": "5.5.x|5.6.x",
        "illuminate/support": "5.5.x|5.6.x",
		"philkra/elastic-apm-php-agent": "dev-master#b8a9140f4be28ce063bd1d1d542d63b87be52914",
		"ramsey/uuid": "^3.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "6.*"
	},
	"repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:georgeboot/elastic-apm-php-agent.git"
        }
    ],
	"autoload" : {
		"psr-4" : {
			"PhilKra\\ElasticApmLaravel\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PhilKra\\Tests\\": "tests/"
		}
	},
	"extra" : {
		"laravel": {
			"providers": [
				"PhilKra\\ElasticApmLaravel\\Providers\\ElasticApmServiceProvider"
			],
			"aliases": {
				"ElasticApm": "PhilKra\\ElasticApmLaravel\\Facades\\ElasticApmFacade"
			}
		}
	},
	"config" : {
		"optimize-autoloader" : true
	},
	"authors" : [
		{
			"name" : "Philip Krauss",
			"email" : "philip@philipkrauss.at",
			"homepage" : "https://github.com/philkra/elastic-apm-laravel"
		},
		{
            "name": "George Boot",
            "email": "george@entryninja.com"
        }
	]
}
